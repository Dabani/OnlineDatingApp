<div class="row">
  <div class="col-sm-8 offset-sm-2 bg-dark-trans my-4 p-3 shad">
    <div class="row">
      
      {{#each userFriends}}      
        {{#each friends}}
          {{#if isFriend}}
          <div class="col-sm-4">
            <div class="card my-3">
              <img src="{{friend.image}}" class="card-img-top img-fluid">
              <div class="card-body">
                <h5 class="card-title text-dark">{{friend.fullname}}</h5>              
                <a href="/userProfile/{{friend._id}}" class="btn btn-primary">Reba Imyirondoro</a>
              </div>
            </div>
          </div>
          {{/if}}
        {{/each}}
      {{/each}}
      
    </div>    
  </div>
</div>